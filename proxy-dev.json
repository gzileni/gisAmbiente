{
  "/geoserver/*": {
    "target": "http://openpuglia.org:8080/",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/geoserver" : "http://openpuglia.org:8080/geoserver"
    }
  },
  "/ambiente/*": {
    "target": "http://localhost:5000/",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/geocode" : "http://localhost:5000/ambiente"
    }
  },
  "/copernicus/*": {
    "target": "http://localhost:5001/",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/reverse" : "http://localhost:5001/copernicus"
    }
  }
}
